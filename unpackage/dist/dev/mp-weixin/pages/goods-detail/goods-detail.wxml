<block wx:if="{{info.title}}"><view class="goods_detail"><swiper indicator-dots autoplay interval="{{2500}}" circular><block wx:for="{{swiperList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><image src="{{item.src}}"></image></swiper-item></block></swiper><view class="goods_info"><view class="box1"><view class="price"><text>{{"￥"+info.sell_price}}</text><text>{{"￥"+info.market_price}}</text></view><view class="goods_name">{{info.title}}</view></view><view class="line"></view><view class="box2"><view>{{"货号："+info.goods_no}}</view><view>{{"库存："+info.stock_quantity}}</view></view><view class="line"></view><view class="box3"><view class="tit">详情介绍</view><view class="content"><rich-text nodes="{{content}}"></rich-text></view></view></view><view class="goods_nav"><uni-goods-nav vue-id="1" fill="{{true}}" options="{{options}}" button-group="{{buttonGroup}}" data-event-opts="{{[['^click',[['optionClick']]],['^buttonClick',[['buttonClick']]]]}}" bind:click="__e" bind:buttonClick="__e" bind:__l="__l"></uni-goods-nav></view></view></block>